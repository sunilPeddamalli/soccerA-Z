<% layout('/layout/boilerplate') %> 

<body>
    <h1>Matches</h1>    
            <ul>
                <% for(match of matches){ %> 
                    <a href="/matches/<%= match._id %> ">
                      <li><%= match.team1 %> <%= match.score1 %> - <%= match.score2 %> <%= match.team2 %></li>
                    </a>
                <% } %> 
            </ul>
</body>
